{
	"EncryptionConfig": {
		"Enabled": true,
		"Type": "aes",
		"TimeConsideration": 1,
		"Threads": 2
	},
	"CompressionConfig": {
		"Enabled": true,
		"Type": "gzip"
	},
	"PoolConfig": {
		"ConnectionPoolConfig": {
			"URI": "amqp://guest:guest@localhost:5672/",
			"ConnectionName": "TurboCookedRabbit",
			"SleepOnErrorInterval": 5000,
			"MaxConnectionCount": 5,
			"Heartbeat": 5,
			"ConnectionTimeout": 10,
			"TLSConfig": {
				"EnableTLS": false,
				"PEMCertLocation": "test/catest.pem",
				"LocalCertLocation": "client/cert.ca",
				"CertServerName": "hostname-in-cert"
			}
		}
	},
	"ConsumerConfigs": {
		"TurboCookedRabbitConsumer": {
			"Enabled": true,
			"QueueName": "ConsumerTestQueue",
			"ConsumerName": "TurboCookedRabbitConsumer",
			"AutoAck": true,
			"Exclusive": false,
			"NoWait": false,
			"QosCountOverride": 5,
			"SleepOnErrorInterval": 1000,
			"SleepOnIdleInterval": 1000
		},
		"TurboCookedRabbitConsumer-Ackable": {
			"Enabled": true,
			"QueueName": "ConsumerTestQueue",
			"ConsumerName": "TurboCookedRabbitConsumer-Ackable",
			"AutoAck": false,
			"Exclusive": false,
			"NoWait": false,
			"QosCountOverride": 10,
			"SleepOnErrorInterval": 100,
			"SleepOnIdleInterval": 0
		}
	},
	"PublisherConfig": {
		"AutoAck": false,
		"SleepOnIdleInterval": 1000,
		"SleepOnErrorInterval": 1000
	}
}